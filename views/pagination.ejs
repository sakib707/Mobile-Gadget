<div>
    <ul class="pagination">

        
        <% for(let i = 1; i <= total_page_count; i++){ %>

            <!-- active_page == i ? 'active' : '' -->
            <li class="page-item <% if(active_page == i){ %> active <% }%>">
                <a href="?page=<%- i -%>" class="page-link">
                    <%- i -%>
                </a>
            </li>
        <% } %>
        
        
    </ul>
        
</div>
    